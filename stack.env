MYSQL_DATABASE=mprov
MYSQL_ROOT_PASSWORD=changemenow
MYSQL_USER=mprov
MYSQL_PASSWORD=changemenow
DB_ENGINE=django.db.backends.mysql
DB_NAME=mprov
DB_USER=mprov
DB_PASS=changemenow
DB_HOST=db
DB_PORT=3306

# the hostname you want to give to the mPCC container.
MPROV_MPCC_HOSTNAME=mprov-mpcc

# IPs that will be used to access the mPCC (other than 127.0.0.1)
MPROV_MPCC_IPs=127.0.0.1

# the fqdn of the mPCC.  This is added to ALLOWED_HOSTS
MPROV_DOMAINNAME=changemenow.com

# the hostname of the jobserver
MPROV_JOBSERVER_HOSTNAME=mprov-jobserver1

# the URL of the mPCC relative to the jobserver.
MPROV_URL=http://172.30.1.1

# apikey will get autogenerated in full stack mode, this WILL be over-written if mpcc is run along
# side the jobserver.
MPROV_APIKEY=changemenow

# the address/hostname of the jobserver.
MPROV_ADDRESS=172.30.1.1

# the mprov jobserver plugins you want to load
MPROV_MODS=repo-update image-update image-delete mprov-webserver dnsmasq libgenders